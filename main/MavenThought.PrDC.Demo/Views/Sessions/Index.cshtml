@using PrairieDevCon.WebSite.Extensions
@using PrairieDevCon.WebSite.ViewModels

@model PrairieDevCon.WebSite.ViewModels.SessionsViewModel

@{
    ViewBag.Title = "Sessions";
}


@foreach (var session in Model.Sessions) { 	
    <div class="session">
        <p class="title">@session.Title</p>
        <p>@session.Abstract</p>
        <p><b>Track:</b>@session.Track</p>	
        <p><b>Style:</b>@session.Style</p>			        
        <p><b>Speaker:</b> <i>@session.Presenter</i></p>
    </div>
}
